/* 代办介绍 */
/* 评价-定时滚动  */
.tab-right {
    width: 95%;
    margin: 0rpx 20rpx 20rpx 20rpx;
}
.right-item {
    height: 100%;
    width: 100%;
    box-sizing: border-box;
}
.tem{
  width: 95%;
  border: 1rpx solid #cecece;
  margin: 0rpx 20rpx 30rpx 20rpx;
  border-radius: 20rpx;
}
.headImg{
  width: 80rpx;
  height: 80rpx;
  margin: 10rpx 30rpx;
  border-radius: 100%;
}
/* 底部按钮  */
.add_btn{
    width: 100%;
    height: 100rpx;
    position: fixed;
    bottom: 0rpx;
    color:white;
    font-weight: bold;
    background-color:#ff7f27;
}
/* 禁止滑动穿透  */
.disableScroll{
  position: fixed; 
  width: 100%;
  height: 100%;
  left: 0;
  bottom: 0;
}
/* 禁止手动滑动  */
.swiper-parent::before {
    content: '';
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background-color: transparent;
    z-index: 1;
}

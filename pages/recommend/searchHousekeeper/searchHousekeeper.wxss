}

.serach-view-text {
  width: 650rpx;
  height: 80rpx;
  padding-left:20rpx;
  border-radius: 10rpx;
  outline: none;
  color: #9e9c9c;
  background-color: white;
  border: 1px solid rgba(0, 0, 0, 0.2);
}

/*列表*/

.templet {
  margin:40rpx 25rpx; 
  border-radius: 20rpx; 
  box-shadow: 2rpx 2rpx 2rpx 2rpx #888888;
}
.headImg{
  width: 80rpx;
  height: 80rpx;
  margin: 40rpx 30rpx;
  border-radius: 100%;
}

/*搜索记录标签*/

.baseInfo_impression_tag {
  border-radius: 10rpx;
  border: 1rpx solid #cecece;
  margin: 0rpx 5rpx 20rpx 20rpx;
  padding: 10rpx 20rpx;
  font-size: 28rpx;
  color: gray;
   z-index: 3;
}

.status{
  position: absolute;
  width: 80rpx;
  height: 80rpx;
  left:-2rpx;
  top:-2rpx;
}


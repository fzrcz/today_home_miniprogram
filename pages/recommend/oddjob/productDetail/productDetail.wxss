/*前半部分样式*/

.flex-content {
  position: relative;
  /* flex: 1; *//* padding: 20rpx 30rpx; */
  text-align: center;
  width: 168rpx;
}

/*后半部分样式*/

.tip {
  position: relative;
  padding: 20rpx 20rpx 10rpx 30rpx;
  flex: 40 0 100rpx;
}

/*中间竖直虚线样式*/

.split-line {
  position: relative;
  flex: 0 0 0;
  margin: 0 0rpx 0 6rpx;
  border-left: 2rpx dashed #bababa;
}

/*给虚线加两个伪类，基本样式如下*/

.split-line:before, .split-line:after {
  content: '';
  position: absolute;
  width: 32rpx;
  height: 16rpx;
  background: #ff5a00;
  left: -18rpx;
  z-index: 1;
}

/*给前半部分加两个伪类，基本样式如下*/

.content:before, .content:after {
  content: '';
  position: absolute;
  /* width: 32rpx; */
  height: 16rpx;
  background: #fff;
  left: -16rpx;
  z-index: 1;
}

/*给前半部分加两个伪类，基本样式如下*/

.tip:before, .tip:after {
  content: '';
  position: absolute;
  /* width: 32rpx; */
  height: 16rpx;
  background: #fff;
  right: -16rpx;
  z-index: 1;
}

.content:before, .tip:before, .split-line:before {
  border-radius: 0 0 16rpx 16rpx;
  top: 0;
}

.content:after, .tip:after, .split-line:after {
  border-radius: 16rpx 16rpx 0 0;
  bottom: 0;
}

